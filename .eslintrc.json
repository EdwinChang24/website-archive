{
    "root": true,
    "env": {
        "browser": true
    },
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "parser": "@typescript-eslint/parser"
    },
    "extends": ["eslint:recommended", "plugin:vue/vue3-recommended", "@nuxtjs/eslint-config-typescript", "prettier"],
    "rules": {
        "vue/multi-word-component-names": "off",
        "vue/no-multiple-template-root": "off"
    }
}
